//TOPIC: LIBRARY
//CODING

package university;
import java.util.Scanner;

public class Library2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner sc = new Scanner(System.in);
		
		String name;
		int studentID;
		int yearCode;
		int daysLate = 0;
		float totalFine;
		String bookTitle;
		String ISBN;
		char excuse;
		float discountedFine = 0;
		int numBooks;
		String excuseDesc;
		float bookFine;
		float sum = 0;
		
		System.out.println("Enter student's name: ");
		name = sc.nextLine();
		System.out.println("Enter student's ID: ");
		studentID = sc.nextInt();
		System.out.println("Enter student's year of study: ");
		yearCode = sc.nextInt();
		
		System.out.println("Does the student have a valid excuse? Enter y or n");
		excuse = sc.next().charAt(0);
		sc.nextLine();
		
		if (excuse == 'y') {
			System.out.println("Enter student's excuse. ");
			excuseDesc = sc.next();
			sc.nextLine();
		}
		else
			System.out.println();
		
		System.out.println("Enter number of books late: ");
		numBooks = sc.nextInt();
		
		System.out.println();
		
		System.out.println("=============================================================");
		System.out.println();
		System.out.println("*Student Info");
		System.out.println("Name: \t \t \t \t" + name);
		System.out.println("Student ID: \t \t \t" + studentID);
		System.out.println("Student's year of study: \t" + yearCode);
		System.out.println();
		System.out.println("*********************");
		System.out.println("*Book Info");
		
		for (int i = 0; i < numBooks; i++) {
			sc.nextLine();
			System.out.println("Enter book's title: ");
			bookTitle = sc.nextLine();
			System.out.println("Enter book's ISBN number: ");
			ISBN = sc.nextLine();
			System.out.println("Enter number of days late for the book: ");
			daysLate = sc.nextInt();
			bookFine = (daysLate*yearCode) + (daysLate*1);
			System.out.println("Book title: \t \t \t" + bookTitle);
			System.out.println("Book ISBN number:  \t \t" + ISBN);
			System.out.println("Fee: \t \t \t \tRM" + bookFine);
			sum = sum + bookFine;
			System.out.println();
		}
		
		if (excuse == 'y') {
			totalFine = sum;
			discountedFine = (float) (totalFine * 0.8);
		}
		
		else
			totalFine = sum;
		
		System.out.println();
		
		if (excuse == 'y') {
			System.out.printf("Total fee is: RM" + "%.2f",discountedFine);
		}
		else
			System.out.println("Total fee is: RM" + totalFine);
		
		System.out.println();
		System.out.println("*********************");
		System.out.println();
		System.out.println("=============================================================");

	}

}

