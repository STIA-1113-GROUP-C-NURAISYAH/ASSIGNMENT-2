//TANG WEI CHIANG (286841)
//TOPIC: COURSE
//CODING

package TOPIC_UNIVERSITY;
import java.util.*;
public class Assignment_2 {

	private static Scanner sc;
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		sc = new Scanner (System.in);
		String name;
		String regc1=null, regc2=null, regc3=null;
		int id, sem;
		int tm1c1=0, tm1c2=0, tm1c3=0;
		int tm2c1=0, tm2c2=0, tm2c3=0;
		int cmc1=00, cmc2=0, cmc3=0;
		int scdLvlCourse=0;
		int minAttRate = 80;
		double minGPA = 2.00;
		int attTime1 = 18, attTime2 = 15, attTime3 = 20;
		int totAttTime = 20;

		//User info
		System.out.print("USER INFO\n");
		System.out.print("Enter your name: ");
		name = sc.nextLine();
		System.out.print("Enter your id: ");
		id = sc.nextInt();
		System.out.print("Enter your semester: ");
		sem = sc.nextInt();
		System.out.println();
		
		int lastsem = sem - 1;
		//registered course
		System.out.print("REGISTERED COURSE:\n");
		System.out.println("How many course did you take in sem " +lastsem+ "?");
		System.out.print("Number of registered course (max.3): ");
		int course = sc.nextInt();
		
		for(int i=1; i<course; i++) { //i = 1 wrong attempt only 
		if(course<=3) {
		switch (course) {
			case 1: {
				System.out.println();
				System.out.print("Enter registered course 1: ");
				regc1 = sc.next();
				System.out.print("Enter Test 1 Marks: ");
				tm1c1 = sc.nextInt();
				System.out.print("Enter Test 2 Marks: ");
				tm2c1 = sc.nextInt();
				System.out.print("Enter Coursework Marks: ");
				cmc1 = sc.nextInt();
			break;
				}
		
			case 2: {
				System.out.println();
				System.out.print("Enter registered course 1: ");
				regc1 = sc.next();
				System.out.print("Enter Test 1 Marks: ");
				tm1c1 = sc.nextInt();
				System.out.print("Enter Test 2 Marks: ");
				tm2c1 = sc.nextInt();
				System.out.print("Enter Coursework Marks: ");
				cmc1 = sc.nextInt();
		
				System.out.print("\nEnter registered course 2: ");
				regc2 = sc.next();
				System.out.print("Enter Test 1 Marks: ");
				tm1c2 = sc.nextInt();
				System.out.print("Enter Test 2 Marks: ");
				tm2c2 = sc.nextInt();
				System.out.print("Enter Coursework Marks: ");
				cmc2 = sc.nextInt();
			break;
				}
		
			case 3: {
				System.out.println();
				System.out.print("Enter registered course 1: ");
				regc1 = sc.next();
				System.out.print("Enter Test 1 Marks: ");
				tm1c1 = sc.nextInt();
				System.out.print("Enter Test 2 Marks: ");
				tm2c1 = sc.nextInt();
				System.out.print("Enter Coursework Marks: ");
				cmc1 = sc.nextInt();
		
				System.out.print("\nEnter registered course 2: ");
				regc2 = sc.next();
				System.out.print("Enter Test 1 Marks: ");
				tm1c2 = sc.nextInt();
				System.out.print("Enter Test 2 Marks: ");
				tm2c2 = sc.nextInt();
				System.out.print("Enter Coursework Marks: ");
				cmc2 = sc.nextInt();
		
				System.out.print("\nEnter registered course 3: ");
				regc3 = sc.next();
				System.out.print("Enter Test 1 Marks: ");
				tm1c3 = sc.nextInt();
				System.out.print("Enter Test 2 Marks: ");
				tm2c3 = sc.nextInt();
				System.out.print("Enter Coursework Marks: ");
				cmc3 = sc.nextInt();
				System.out.println(); 
			break;
			}
		}
	}
		else {
			System.out.print("Please enter a valid number (1-3).\n");
			System.out.print("REGISTERED COURSE:\n");
			System.out.println("How many course did you take in sem " +lastsem+ "?");
			System.out.print("Number of registered course (max.3): ");
			course = sc.nextInt();
			}
		}
		
		
		//Second-level course registration and course's requirements 	
		String c1 = "STIA1123";
		String c2 = "STID3113";
		String c3 = "STQM2103";
		boolean invalid = true;
        while (invalid){
		System.out.print("Enter the second-level course you want to register: \n");
		System.out.println("Press:\n (1) STIA1123 \n (2) STID3113 \n (3) STQM2103");
		System.out.print("Your answer: \n");
		scdLvlCourse = sc.nextInt();
		
		switch (scdLvlCourse) {
		case 1: {
			System.out.println(c1 + " requirements: ");
			System.out.println("Completed "+regc1+ " with GPA " +String.format("%.2f",minGPA)+ 
							" and attendance rate " +minAttRate+ "% or above.");
			System.out.println(); 
			invalid = false;
			break;
			}
		
		case 2: {
			System.out.println(c2 + " requirements: ");
			System.out.println("Completed "+regc2+" with GPA " +String.format("%.2f",minGPA)+ 
							" and attendance rate " +minAttRate+ "% or above.");
			System.out.println(); 
			invalid = false;
			break;
			}
		
		case 3: {
			System.out.println(c3 + " requirements: ");
			System.out.println("Completed "+regc3+" with GPA " +String.format("%.2f",minGPA)+ 
							" and attendance rate " +minAttRate+ "% or above.");
			System.out.println(); 
			invalid = false;
			break;
			}	
		
		default:
			System.out.println("Invalid course.");
			System.out.print("Please re-enter a valid course.\n");
			System.out.println();
			}
		}
 
        //Formula
      	int tMark1 = tm1c1 + tm2c1;
      	int totMark1 = tMark1 + cmc1;
      	double att1 = (double) attTime1 / totAttTime;
      	double attRate1 = att1 * 100;
      	double GPA1=0;   	
      	if (totMark1>=80)
      		GPA1 = 4.00;
      	if ((totMark1>=75) && (totMark1<=79))
      		GPA1 = 3.67;
      	if ((totMark1>=70) && (totMark1<=74))
      		GPA1 = 3.33;
      	if ((totMark1>=65) && (totMark1<=69))
      		GPA1 = 3.00;
      	if ((totMark1>=60) && (totMark1<=64))
      		GPA1 = 2.67;
      	if ((totMark1>=55) && (totMark1<=59))
      		GPA1 = 2.33;
      	if ((totMark1>=50) && (totMark1<=54))
      		GPA1 = 2.00;
      	if ((totMark1>=45) && (totMark1<=49))
      		GPA1 = 1.67;
      	if ((totMark1>=40) && (totMark1<=44))
      		GPA1 = 1.33;
      	if ((totMark1>=35) && (totMark1<=39))
      		GPA1 = 1.00;
      	if (totMark1<=34)
      		GPA1 = 0.00;
      	
      	int tMark2 = tm1c2 + tm2c2;
      	int totMark2 = tMark2 + cmc2;
      	double att2 = (double) attTime2 / totAttTime;
      	double attRate2 = att2 * 100;
      	double GPA2=0;
      	if (totMark2>=80)
      		GPA2 = 4.00;
      	if ((totMark2>=75) && (totMark2<=79))
      		GPA2 = 3.67;
      	if ((totMark2>=70) && (totMark2<=74))
      		GPA2 = 3.33;
      	if ((totMark2>=65) && (totMark2<=69))
      		GPA2 = 3.00;
      	if ((totMark2>=60) && (totMark2<=64))
      		GPA2 = 2.67;
      	if ((totMark2>=55) && (totMark2<=59))
      		GPA2 = 2.33;
      	if ((totMark2>=50) && (totMark2<=54))
      		GPA2 = 2.00;
      	if ((totMark2>=45) && (totMark2<=49))
      		GPA2 = 1.67;
      	if ((totMark2>=40) && (totMark2<=44))
      		GPA2 = 1.33;
      	if ((totMark2>=35) && (totMark2<=39))
      		GPA2 = 1.00;
      	if (totMark2<=34)
      		GPA2 = 0.00;
      	
      	int tMark3 = tm1c3 + tm2c3;
      	int totMark3 = tMark3 + cmc3;
      	double att3 = (double) attTime3 / totAttTime;
      	double attRate3 = att3 * 100;
      	double GPA3=0;
      	if (totMark3>=80) 
      		GPA3 = 4.00;
      	if ((totMark3>=75) && (totMark3<=79))
      		GPA3 = 3.67;
      	if ((totMark3>=70) && (totMark3<=74))
      		GPA3 = 3.33;
      	if ((totMark3>=65) && (totMark3<=69))
      		GPA3 = 3.00;
      	if ((totMark3>=60) && (totMark3<=64))
      		GPA3 = 2.67;
      	if ((totMark3>=55) && (totMark3<=59))
      		GPA3 = 2.33;
      	if ((totMark3>=50) && (totMark3<=54))
      		GPA3 = 2.00;
      	if ((totMark3>=45) && (totMark3<=49))
      		GPA3 = 1.67;
      	if ((totMark3>=40) && (totMark3<=44))
      		GPA3 = 1.33;
      	if ((totMark3>=35) && (totMark3<=39))
      		GPA3 = 1.00;
      	if (totMark3<=34)
      		GPA3 = 0.00;
      	
		System.out.println("*******************************************************************");
		//Display User Info
		System.out.println("-------------------------------------------------------------------");
		System.out.println("\t\t\t   USER INFO");
		System.out.println("-------------------------------------------------------------------");
		System.out.println("Name    				: " +name);
		System.out.println("ID      				: " +id);
		System.out.println("Semester				: " +sem);
		System.out.println();
				
		//Display GPA of the registered course
		if(scdLvlCourse == 1) {
			System.out.println("-------------------------------------------------------------------");
			System.out.println("\t\t\t" +regc1+ " RESULT");
			System.out.println("-------------------------------------------------------------------");
			System.out.println("Test 1					: " +tm1c1+ "%");
			System.out.println("Test 2					: " +tm2c1+ "%");
			System.out.println("Tests marks				: " +tMark1+ "%");
			System.out.println("Coursework marks			: " +cmc1+ "%");
			System.out.println("Total marks				: " +totMark1+ "%");
			System.out.printf("GPA					: %.2f",GPA1);
			System.out.println();
			
			//Display attendance rate of the registered course
			System.out.println("Attended times				: " +attTime1+ " days");
			System.out.println("Total Attended times needed 		: " +totAttTime+ " days");
			System.out.println("Attendance rate				: " +attRate1+ "%");
			System.out.println();
					
			//Display whether the student qualify for the second-level course registration or not
			System.out.println("-------------------------------------------------------------------");
			System.out.println("\t\t" +c1+ " REGISTRATION CONFIRMATION");
			System.out.println("-------------------------------------------------------------------");
				if ((GPA1>=2.00) && (attRate1>=80)) {
					System.out.print("You've passed the GPA and attendance rate requirements.\n");
					System.out.print(c1+ " registered sucessfully!\n");
				}
			
				else if ((GPA1>=2.00) && (attRate1<=79)) {
					System.out.print("You've passed the GPA requirement but failed to fulfilled "
								+ "attendance rate, please retake the course.\n");
				}
			
				else if ((GPA1<=1.99) && (attRate1>=80)) {
					System.out.print("You've passed the attendance rate requirement but failed to "
								+ "fulfilled GPA, please retake the course.\n");
				}
				else {
					System.out.print("You've failed both GPA and attendance rate requirements, "
								+ "please retake the course.\n");
				}
			}
		
		else if(scdLvlCourse == 2) {
			System.out.println("-------------------------------------------------------------------");
			System.out.println("\t\t\t" +regc2+ " RESULT");
			System.out.println("-------------------------------------------------------------------");
			System.out.println("Test 1					: " +tm1c2+ "%");
			System.out.println("Test 2					: " +tm2c2+ "%");
