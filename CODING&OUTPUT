			System.out.println();
					
			//Display whether the student qualify for the second-level course registration or not
			System.out.println("-------------------------------------------------------------------");
			System.out.println("\t\t" +c2+ " REGISTRATION CONFIRMATION");
			System.out.println("-------------------------------------------------------------------");
				if ((GPA2>=2.00) && (attRate2>=80)) {
					System.out.print("You've passed the GPA and attendance rate requirements.\n");
					System.out.print(c2+ " registered sucessfully!\n");
				}
			
				else if ((GPA2>=2.00) && (attRate2<=79)) {
					System.out.print("You've passed the GPA requirement but failed to fulfilled "
								+ "attendance rate, please retake the course.\n");
				}
			
				else if ((GPA2<=1.99) && (attRate2>=80)) {
					System.out.print("You've passed the attendance rate requirement but failed to "
